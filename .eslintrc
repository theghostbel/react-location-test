{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true
  },
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "amd": true
  },
  "globals": {},
  "rules": {
    "semi": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "no-undef": 2,
    "eqeqeq": 2,
    "curly": 0,
    "no-console": 1,
    "no-unused-vars": 1
  },
}
