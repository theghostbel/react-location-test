{
  "ecmaFeatures": {
    "jsx": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "generators": true
  },
  "rules": {
    "semi": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "no-undef": 2
  },
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "amd": true
  },
  "globals": {}
}
